{{>layout/items/header}}

<h1>ITEMS Register</h1>

<div class="col-md-12">
    <div class="col-md-4">

        <form>
            <div class="form-group">
                <label for="title">제목</label>
                <input type="text" class="form-control" id="title" placeholder="제목을 입력하세요">
            </div>
            <div class="form-group">
                <label for="code">code</label>
                <input type="text" class="form-control" id="code" placeholder="제목을 입력하세요">
            </div>
            <div class="form-group">
                <label for="price">price</label>
                <input type="text" class="form-control" id="price" placeholder="제목을 입력하세요">
            </div>
            <div class="form-group">
                <label for="contributor">contributor</label>
                <input type="text" class="form-control" id="contributor" placeholder="제목을 입력하세요">
            </div>
            <div class="form-group">
                <label for="content"> 내용 </label>
                <textarea class="form-control" id="content" placeholder="내용을 입력하세요"></textarea>
            </div>
        </form>

        <a href="/" role="button" class="btn btn-secondary">취소</a>
        <button type="button" class="btn btn-primary" id="items-btn-save">등록</button>
    </div>
</div>

{{>layout/items/footer}}